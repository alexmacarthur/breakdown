{
    "name": "picperf/breakdown",
    "version": "0.0.7",
    "description": "A little CLI tool for breaking down an HTTP request.",
    "keywords": [
        "http",
        "curl"
    ],
    "homepage": "https://picperf.io",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/alexmacarthur/breakdown/issues",
        "source": "https://github.com/alexmacarthur/breakdown"
    },
    "authors": [
        {
            "name": "Alex MacArthur"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/support": "^12.18",
        "nunomaduro/termwind": "^2.3",
        "symfony/console": "^7.3",
        "symfony/http-foundation": "^7.3",
        "twig/twig": "^3.21"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1",
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "PicPerf\\Breakdown\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "breakdown"
    ]
}
