{
    "name": "picperf/breakdown",
    "version": "0.0.6",
    "description": "A little CLI tool for breaking down an HTTP request.",
    "keywords": [
        "http",
        "curl"
    ],
    "homepage": "https://picperf.io",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/alexmacarthur/breakdown/issues",
        "source": "https://github.com/alexmacarthur/breakdown"
    },
    "authors": [
        {
            "name": "Alex MacArthur"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "illuminate/http": "^12.0",
        "illuminate/view": "^12.0",
        "laravel-zero/framework": "dev-feature/laravel-12",
        "nunomaduro/termwind": "^2.3",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/http-foundation": "^7.3"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "autoload": {
        "psr-4": {
            "PicPerf\\Breakdown\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "breakdown"
    ]
}
